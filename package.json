    "astro": "^4.5.9",
    "astro-font": "^0.0.77",
    "astro-icon": "^1.1.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.2"
  }
}
